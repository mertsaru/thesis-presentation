\defbeamertemplate*{footline}{hsmw}
{
	% Raise the depth of the footline boxes depending on the existance of the progressbar (setting the depth for one box is sufficient to raise all others)
	\iftoggle{option@progressbar}{\def\raiseDepth{.185cm}}{\def\raiseDepth{.235cm}}

	\leavevmode%
	\usebeamertemplate*{page number in head/foot}%
	%TODO: Using parbox instead of vbox produces a small vertical gap...
%	\parbox[b]{9.5cm}{%
%		\begin{beamercolorbox}[wd=9.5cm, ht=.2cm, dp=.1cm]{footline title}%
%			\usebeamerfont{footline title}%
%			\ifdisplayablecontent{\insertshorttitle}{\insertshorttitle{} | }{}%
%			\insertcurrentspeaker{}%
%		\end{beamercolorbox}%
%	%
%		\begin{beamercolorbox}[wd=9.5cm, ht=.2cm, dp=.12cm,]{footline date} 
%			\usebeamerfont{footline date}%
%			\parbox[b][.2cm][t]{\linewidth}{\copyright{} \insertshortdate{} Hochschule Mittweida}
%		\end{beamercolorbox}%
%	}%
	\vbox%
	{%
		\begin{beamercolorbox}[wd=9.5cm, ht=.2cm, dp=.1cm]{footline title} %
			\usebeamerfont{footline title}%
			\ifdisplayablecontent{\insertshorttitle}{\insertshorttitle{} | }{}%
			\insertcurrentspeaker{}%
		\end{beamercolorbox}%
		%
		\begin{beamercolorbox}[wd=9.5cm, ht=.2cm]{footline date} 
			\usebeamerfont{footline date}%
			\parbox[b][.2cm][t]{\linewidth}{\copyright{} \insertshortdate{} Hochschule Mittweida}
		\end{beamercolorbox}%
	}%
	%
	\begin{beamercolorbox}[wd=5cm, ht=.5cm, dp=\raiseDepth, right, rightskip=1cm]{footline faculty} % CD page 16 and 17
		\usebeamerfont{footline faculty}
		
		\leavevmode%
		\ifdisplayablecontent{\insertshortinstitute}
		{%
			\parbox[b][.3cm][t]{2cm}{\raggedleft\insertshortinstitute}%
			\hspace*{.12cm}%
			\rule{.15pt}{4mm}% .15pt = .0529 mm (scaled according to CD)
			\hspace*{.12cm}%
		}{}%
		%
		\usebeamertemplate*{footline logo}% 1.76 x 0.44
	\end{beamercolorbox}%
	
	\iftoggle{option@progressbar}
	{%
		\stepcounter{progressPage}%
		%
		\ifnum\value{progressPage}<1\else %%%% prevents overfull hbox on titlepage, generated by boxes of negativ width ;)
			\pgfmathtruncatemacro{\progressValueCurrent}{\value{progressPage}}%
			\pgfmathtruncatemacro{\progressValueTotal}{\GetProgressbarLastSlide}% \insertmainframenumber - 1 % \GetLastSlideNumber
			\pgfmathtruncatemacro{\progressValueFuture}{\progressValueTotal - \progressValueCurrent}%
			\pgfmathsetmacro{\progressFactorPast}{(\progressValueCurrent - 1) / \progressValueTotal}%
			\pgfmathsetmacro{\progressFactorCurrent}{1 / \progressValueTotal}%
			\pgfmathsetmacro{\progressFactorFuture}{\progressValueFuture / \progressValueTotal}%
			%
			\leavevmode%
			\begin{beamercolorbox}[wd=\progressFactorPast\paperwidth, ht=.05cm]{progressbar past}\end{beamercolorbox}%
			\begin{beamercolorbox}[wd=\progressFactorCurrent\paperwidth, ht=.05cm]{progressbar current}\end{beamercolorbox}%
			\begin{beamercolorbox}[wd=\progressFactorFuture\paperwidth, ht=.05cm]{progressbar future}\end{beamercolorbox}%
		\fi
	}{}
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Notes

\defbeamertemplate*{note page}{hsmw}
{
	\pgfmathsetmacro{\frameScaleFactor}{\beamer@headheight / \paperheight}%	
	
	% Frame title
	\begin{textblock}{16}(0, 0)	
		\begin{beamercolorbox}[wd=\linewidth, ht=\beamer@headheight]{note page watermark} 
			\parbox[c][\beamer@headheight]{1 cm}
			{
				\usebeamerfont{note page watermark}
				\hfil\rotatebox[origin=c]{90}{\resizebox{.9\beamer@headheight}{!}{Notizen}}\hfil
			}%
			\parbox[c][\beamer@headheight][b]{\dimexpr\paperwidth - \frameScaleFactor\paperwidth - 1cm\relax}
			{
				\vspace*{.5 cm}
				\usebeamerfont{frametitle}
				\autofontscaledownbox*{\linewidth}{.7cm}{\insertframetitle}
				
				\iftoggle{option@framesubtitle}
				{
					\vspace*{.2 cm}
					\usebeamerfont{framesubtitle}%
					\autofontscaledownbox*{\linewidth}{.4cm}{\insertframesubtitle}
				}{\vspace*{.4 cm}}
			}%
			\parbox[c][\beamer@headheight]{\frameScaleFactor\paperwidth}{\insertslideintonotes{\frameScaleFactor}}%
			
			\nointerlineskip
			\color{hsmw}\rule{\linewidth}{\arrayrulewidth}
		\end{beamercolorbox}
	\end{textblock}	
	
	% Note content
	\vspace*{\beamer@headheight}
	\insertnote
	
	\begin{textblock}{16}(0, 8)
		\begin{beamercolorbox}[wd=\linewidth, ht=1cm, sep=.2cm, right]{note page watermark} 
			\usebeamerfont{note page pagenumber}
			\insertframenumber
			\iftoggle{option@totalpages}{/\insertmainframenumber}{}
		\end{beamercolorbox}
	\end{textblock}
}

