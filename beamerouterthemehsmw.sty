%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Progressbar

\iftoggle{option@progressbar}
{% Count pages with separate counter, due to the implemented counters in beamer do not seem to be sufficient
	% Save current slide counter on thank you slide to the aux file
	\AddToHook{cmd/makethankyou/after}{\immediate\write\@auxout{\string\gdef\string\progressbar@thankyou@slide{\arabic{progressPage}}}}
	% Save current slide counter on appendix to the aux file
	\AddToHook{cmd/appendix/after}{\immediate\write\@auxout{\string\gdef\string\progressbar@appendix@slide{\arabic{progressPage}}}} % \g@addto@macro{\appendix}
	% Save current slide counter on end of document to the aux file
	\AddToHook{enddocument/afterlastpage}{\immediate\write\@auxout{\string\gdef\string\progressbar@last@slide{\arabic{progressPage}}}} % \AtEndDocument
	
	% Retrieve the last progressed page from aux file or return 1 as intermediate (double run needed)
	\newcommand{\GetProgressbarLastSlide}
	{
		\ifnum\Identify@Progressbar@LastSlide<1 % Possible result in draft mode (\stepcounter{progressPage} only called once): Division by zero is apparently not a good thing
			1
		\else
			\Identify@Progressbar@LastSlide
		\fi
	}
	\newcommand{\Identify@Progressbar@LastSlide}
	{
		\ifdefined\progressbar@last@slide
			\ifdefined\progressbar@thankyou@slide % If thankyou is present, take it as last slide
				\progressbar@thankyou@slide
			\else
				\ifdefined\progressbar@appendix@slide % If appendix is present, take it as last slide
					\progressbar@appendix@slide
				\else % Use actual last slide if there is no appendix
					\progressbar@last@slide
				\fi
			\fi
		\else
			1 % Return 1 on first run (placeholder)
		\fi
	}
	
	\newcounter{progressPage} % Will be used for tracking real content pages based on display of footline (not plain frames)
	\setcounter{progressPage}{-1} % Adjust counter for titlepage
}{}

